public class GFG {
    // Starting counter
    int counter = 1;
    static int N;
    // Function to print odd numbers
    public void printOddNumber()
    {
        synchronized (this)
        {
            // Print number till the N
            while (counter < N) {
                // If count is even then print
                while (counter % 2 == 0) {
                    // Exception handle
                    try {
                        wait();
                    }
                    catch (InterruptedException e) {
                        e.printStackTrace();
                    }
                }
                // Print the number
                System.out.print(counter + " ");
                // Increment counter
                counter++;
                // Notify to second thread
                notify();
            }
        }
    }
    
    
